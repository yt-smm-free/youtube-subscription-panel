<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YouTube Subscription Panel</title>
    
    <!-- Base Styles -->
    <link rel="stylesheet" href="/css/style.css">
    
    <!-- Theme Styles (Choose one) -->
    <link rel="stylesheet" href="/css/futuristic.css">
    <link rel="stylesheet" href="/css/futuristic-icons.css">
    <link rel="stylesheet" href="/css/futuristic-loader.css">
    <link rel="stylesheet" href="/css/modern-dark.css">
    
    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Share+Tech+Mono&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;500;700;900&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;500;700&family=Inter:wght@300;400;500;700&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@400;500&display=swap">
    
    <!-- Google API -->
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <% if (typeof meta !== 'undefined' && meta.googleClientId) { %>
    <meta name="google-signin-client_id" content="<%= meta.googleClientId %>">
    <% } %>
</head>
<body>
    <div class="container">
        <!-- <header>
            <div class="logo">
                <h1>YT<span>BOOST</span></h1>
            </div>
            <% if (typeof showNav === 'undefined' || showNav) { %>
            <nav>
                <ul>
                    <li><a href="/">Home</a></li>
                    <li><a href="/abc/xxx/dashboard">Dashboard</a></li>
                    <li><a href="/abc/xxx/campaigns">Campaigns</a></li>
                    <li><a href="/abc/xxx/users">Users</a></li>
                    <li><a href="/abc/xxx/logout">Logout</a></li>
                </ul>
            </nav>
            <% } %>
        </header>
         -->
        <main>
            <%- typeof body !== 'undefined' ? body : '' %>
        </main>
        
        <footer>
            <p>&copy; <%= new Date().getFullYear() %> YouTube Subscription Panel. All rights reserved.</p>
        </footer>
    </div>
    
    <!-- Core Scripts -->
    <script src="/js/main.js"></script>
    
    <!-- Theme Scripts -->
    <script src="/js/futuristic-effects.js"></script>
    <script src="/js/futuristic-loader.js"></script>
    <script src="/js/modern-dark.js"></script>
    
    <!-- Additional Scripts -->
    <% if (typeof scripts !== 'undefined') { %>
        <% scripts.forEach(function(script) { %>
            <script src="<%= script %>"></script>
        <% }); %>
    <% } %>
</body>
</html>
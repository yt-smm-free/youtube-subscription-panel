<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YouTube Subscription Panel</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Share+Tech+Mono&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;500;700&display=swap">
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <% if (typeof meta !== 'undefined' && meta.googleClientId) { %>
    <meta name="google-signin-client_id" content="<%= meta.googleClientId %>">
    <% } %>
</head>
<body>
    <div class="container">
        <header>
            <div class="logo">
                <h1>YT<span>BOOST</span></h1>
            </div>
            <% if (typeof showNav !== 'undefined' && showNav) { %>
            <nav>
                <ul>
                    <li><a href="/">Home</a></li>
                    <li><a href="/admin/dashboard">Dashboard</a></li>
                    <li><a href="/admin/campaigns">Campaigns</a></li>
                    <li><a href="/admin/users">Users</a></li>
                    <li><a href="/admin/logout">Logout</a></li>
                </ul>
            </nav>
            <% } %>
        </header>
        
        <main>
            <%- body || '' %>
        </main>
        
        <footer>
            <p>&copy; <%= new Date().getFullYear() %> YouTube Subscription Panel. All rights reserved.</p>
        </footer>
    </div>
    
    <script src="/js/main.js"></script>
    <% if (typeof scripts !== 'undefined') { %>
        <% scripts.forEach(function(script) { %>
            <script src="<%= script %>"></script>
        <% }); %>
    <% } %>
</body>
</html>